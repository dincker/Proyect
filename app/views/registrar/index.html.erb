<h1>Nuevo Usuario</h1>
	<%= form_for :post do |f| %>		
	<p>
		<%= f.label :Nombre %><br>
		<%= f.text_field :nombre %>
	</p>
	<p>
		<%= f.label :Apellido %><br>
		<%= f.text_field :apellido %>
	</p>
	<p>
		<%= f.label :Telefono %><br>
		<%= f.text_field :telefono %>
	</p>
	<p>
		<%= f.label :Email %><br>
		<%= f.text_field :email %>
	</p>
	<p>
		<%= f.label :Direccion %><br>
		<%= f.text_field :direccion %>
	</p>
	<p>
		<%= f.label :Password %><br>
		<%= f.text_field :password %>
	</p>
	<p>
		<%= f.label :Confirmar_password%><br>
		<%= f.text_field :confirmar_password %>
	</p>
	<p>
    <%= f.submit 'crear'%>
  	</p>
	<% end %>

